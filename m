# /etc/lilo.conf - See: `lilo(8)' and `lilo.conf(5)',
#
# Support LBA for large hard disks.
#
lba32
#
boot=/dev/hda

# Specifies the device that should be mounted as root. (`/')
#
root=/dev/hda2
# compact

# Installs the specified file as the new boot sector
#
install=menu

# Specifies the location of the map file
#
map=/boot/map
# Specifies the VGA text mode at boot time. (normal, extended, ask, <mode>)
#
# vga=ask
# vga=9
#
#vga=normal
vga=791

append="hdc=ide-scsi console=/dev/tty6"

image=/vmlinuz      # /boot/vmlinux-2.6.0
        label=Linux
        read-only
#       restricted
        alias=1

image=/vmlinuz-old # /boot/vmlinuz-2.4.23-ck1
        label=Old
        read-only
#       restricted
        alias=2

image=/vmlinuz-older # /boot/vmlinuz-2.4.18
        label=Older
        read-only
#       restricted

----------------------------------------------------------------------
