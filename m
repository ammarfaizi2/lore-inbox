-		"lea %0,%%eax\n\t"
-		"call __up_wakeup\n"
-		"1:"
+		"jle 2f\n"
+		"1:\n"
+		LOCK_SECTION_START("")
+		"2:\tlea %0,%%eax\n\t"
+		"call __up_wakeup\n\t"
+		"jmp 1b\n"
+		LOCK_SECTION_END
+		".subsection 0\n"
 		:"+m" (sem->count)
 		:
 		:"memory","ax");
